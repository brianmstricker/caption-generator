{
 "compilerOptions": {
  "target": "es2016",
  "module": "Preserve",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "outDir": "./dist",
  "moduleDetection": "force",
  "resolveJsonModule": true,
  "allowJs": true,
  "isolatedModules": true
 },
 "exclude": ["node_modules"]
}
